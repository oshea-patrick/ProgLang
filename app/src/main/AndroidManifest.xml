<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proglang">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".songQueueActivity"></activity>
        <activity android:name=".songQueue" />
        <activity
            android:name=".SongQueue"
            android:label="@string/title_activity_song_queue"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".songQueueList"
            android:label="@string/title_activity_song_queue_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.login.StartScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>